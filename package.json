{
    "name": "env-vault",
    "version": "0.1.0",
    "private": true,
    "main": "dist-electron/electron.main.js",
    "scripts": {
        "dev": "vite",
        "electron:watch": "tsc --watch",
        "electron:dev": "ELECTRON_DISABLE_SANDBOX=1 concurrently -k \"vite\" \"tsc -p tsconfig.electron.json --watch\" \"wait-on dist-electron/electron.main.js && electron dist-electron/electron.main.js\"",
        "build": "vite build",
        "electron:build": "tsc --project tsconfig.json",
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "devDependencies": {
        "@types/electron": "^1.4.38",
        "@types/node": "^25.0.2",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "autoprefixer": "^10.4.23",
        "concurrently": "^9.2.1",
        "electron": "^39.2.7",
        "electron-builder": "^26.0.12",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "wait-on": "^9.0.3"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.11.2",
        "@types/libsodium-wrappers-sumo": "^0.7.8",
        "@vitejs/plugin-react": "^5.1.2",
        "argon2": "^0.44.0",
        "cross-spawn": "^7.0.6",
        "libsodium-wrappers-sumo": "^0.7.15",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-icons": "^5.5.0",
        "react-redux": "^9.2.0"
    }
}
