{
    "name": "env-vault",
    "version": "0.1.0",
    "private": true,
    "main": "dist-electron/main.js",
    "scripts": {
        "dev": "vite",
        "electron:build": "tsc -p tsconfig.electron.json",
        "electron:watch": "tsc -p tsconfig.electron.json --watch",
        "electron:dev": "npm run electron:build && concurrently -k \"npm run dev\" \"npm run electron:watch\" \"wait-on http://localhost:5173 && npx electron --no-sandbox dist-electron/electron/main.js\"",
        "build": "vite build",
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "devDependencies": {
        "@tailwindcss/line-clamp": "^0.4.4",
        "@types/node": "^25.0.2",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "autoprefixer": "^10.4.23",
        "concurrently": "^9.2.1",
        "electron": "^39.2.7",
        "electron-builder": "^26.0.12",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.9.3",
        "vite": "^5.2.0",
        "wait-on": "^9.0.3"
    },
    "dependencies": {
        "@monaco-editor/react": "^4.7.0",
        "@reduxjs/toolkit": "^2.11.2",
        "lucide-react": "^0.562.0",
        "monaco-editor": "^0.55.1",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-icons": "^5.5.0",
        "react-redux": "^9.2.0"
    }
}
